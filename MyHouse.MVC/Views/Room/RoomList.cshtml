                )
            }
            else {
                let ImageNames = upload_img(Images);
                for (let i = 0; i < ImageNames.length; i++) {
                    ImageNames[i] = '@Model.Domain' + ImageNames[i];
                }
                let StringImageNames = ImageNames.join(';');
                console.log(StringImageNames);
                $(this).prop('disabled', true);
                Insert(id, name, branchId, branchFloor, price, acreage, deposit, bedamount, StringImageNames);

            }

