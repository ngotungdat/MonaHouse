    <script>
        function DetailImage(RoomID) {
            console.log('call api');
            $.ajax({
                type: 'GET',
                dataType: "json",
                url: '@Model.Domain' + 'api/RoomImage/GetImageRoomByRoomID?RoomId=' + RoomID,
                headers: {
                    'Authorization': 'Bearer ' + '@Model.Token',
                    'Content-Type': 'application/json'
                },
                success: function (data, status, xhr) {
                    let list = data.Data;
                    console.log('list: ', list);
                    let html = '';
                    for (let i = 0; i < list.length; i++) {
                        let item = list[i];
                        html += '<img src="' + item.Link +'" alt="image" style=" margin-left: 20px; margin-right: 20px;" width="150" height="200" />';
                        
                    }
                    let a = $('#image-byroomid').html(html);
                    console.log(a);
                    $('#modal-room-image').modal('show');
                },
                error: function (xhr, status, error) {
                    alert("Result: " + status + " " + error + " " + xhr.status + " " + xhr.statusText)
                }
            });
        }
    </script>
