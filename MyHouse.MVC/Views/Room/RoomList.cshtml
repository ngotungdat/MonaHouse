                Swal.fire(
                    'Cảnh báo',
                    'Vui lòng nhập đúng số điện nước!',
                    'warning'
                )
            }
            else {
                let listImage = [];
                let data = {};
                if (img_electric != null || img_water != null) {
                    listImage.push(img_electric);
                    listImage.push(img_water);
                    let ImageNames = upload_img(listImage);
