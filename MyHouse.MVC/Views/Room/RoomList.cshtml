                    $(this).prop('disabled', false);

                    $('#js-electronicNote').modal('hide');
                    load_room_detail(PageIndex, PageSize, SearchContent, OrderBy, Status, RoomType, BranchId, Electric, TakeMoney, DebtMoney);
                });
            }
        })

