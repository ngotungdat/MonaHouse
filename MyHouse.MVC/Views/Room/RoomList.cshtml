                            status = "Sắp chuyển vào";
                        }if (item.Status == 2) {
                            CssClass="badge badge-primary wd-100 pd-5 tx-12";
                            status = "Đang thuê";
                        } if (item.Status == 3) {
                            CssClass="badge badge-danger wd-100 pd-5 tx-12";
                            status = "Sắp dọn ra";
                        } if (item.Status == 4) {
                            CssClass = "badge badge-danger wd-100 pd-5 tx-12";
                            status = "Đang sửa";
                        } if (item.Status == 5) {
                            CssClass = "badge badge-danger wd-100 pd-5 tx-12";
                            status = "Đã khóa";
                        }
                        // nếu chưa có ngày dọn vào => phòng trống
                        let date_into_room = item.DateInToRoom;
                        if (date_into_room == null) {
                            date_into_room = 'chưa có ngày dọn vào';
                        }
                        else {
                            date_into_room = format_date(item.DateInToRoom);
                        }
                        // nếu không có người đại diện
                        let user_name = item.FullName;
                        let user_phone = item.Phone;
                        let user_debt = item.DebtMoney;
                        if (user_name == null) {
                            user_name = 'Chưa có người đại diện';
                            user_phone = 'chưa có số điện thoại';
                            user_debt = 0;
                        }
                        //#region nội dung html
                        html += `
                            <div class="col-md-3 col-sm-6 item">
                                <div class="card item-card card-block">
                                    <h4 class="card-title text-right">
                                        <div class="dropdown__wrap">
                                            <a href="#" class="table-action">
                                                <i class="material-icons" aria-hidden="true">settings</i>
                                            </a>
                                            <ul class="drop-menu">
                                                    <li class="drop-menu-item"><a href="./RoomDetail/`+ item.Id + `" class="item-link"><span class="icon"><i class="fas fa-info-circle" aria-hidden="true"></i></span>Chi tiết</a></li>
                                                    <li class="drop-menu-item">
                                                        <a href="#js-electronicNote" data-toggle="modal" class="item-link" onclick="electricRecord(`+ item.Id + `)">
                                                            <span class="icon"><i class="fas fa-calendar-alt" aria-hidden="true"></i></span>Ghi
                                                            điện
                                                        </a>
                                                    </li>
                                                    <li class="drop-menu-item">
                                                        <a href="#js-ImageNote" data-toggle="modal" class="item-link" onclick="DetailImage(`+ item.Id + `)">
                                                            <span class="icon"><i class="fas fa-image" aria-hidden="true" ></i></span>Hình ảnh
                                                        </a>
                                                    </li>
                                                    <li class="drop-menu-item">
                                                        <a href="#js-ImageNote" data-toggle="modal" class="item-link" onclick="EditRoom(`+ item.Id + `)">
                                                            <span class="icon"><i class="fas fa-edit" aria-hidden="true"></i></span>Chỉnh sửa
                                                        </a>
                                                    </li>
                                                    <li class="drop-menu-item">
                                                        <a href="javascript:;" data-toggle="modal" class="item-link" onclick="CalMoney(`+ item.Id + `)">
                                                            <span class="icon"><i class="fas fa-hand-holding-usd" aria-hidden="true" ></i></span>Tính tiền
                                                        </a>
                                                    </li>
                                                    <li class="drop-menu-item">
                                                        <a href="javascript:;" data-toggle="modal" class="item-link" onclick="takeMoney(`+ item.Id + `)">
                                                            <span class="icon"><i class="fas fa-hand-holding-usd" aria-hidden="true" ></i></span>Thu
                                                            tiền
                                                        </a>
                                                    </li>
                                                    <li class="drop-menu-item">
                                                        <a href="javascript:;" data-toggle="modal" class="item-link" onclick="goOut(`+ item.Id + `)">
                                                            <span class="icon"><i class="fas fa-sign-out-alt" aria-hidden="true" onclick="moveOut(`+ item.Id + `)"></i></span>Dọn
                                                            ra
                                                        </a>
                                                    </li>
                                                    <li class="drop-menu-item">
                                                        <a href="javascript:;" class="item-link" onclick="goInRoom(` + item.Id + `)">
                                                            <span class="icon"><i class="fas fa-sign-in-alt" aria-hidden="true"></i></span>Dọn vào
                                                        </a>
                                                    </li>
                                                </ul>
                                        </div>
                                    </h4>
                                    <img height:150px; width:100%; src="https://static.pexels.com/photos/7096/people-woman-coffee-meeting.jpg" alt="Photo of sunset">
                                    <h5 class="item-card-title mt-3 mb-3">` + item.BranchName + ` • ` + item.Name + ` </h5>
                                    <p>Giá tiền: <span class="tx-success"> `+ formatter.format(item.Price) + `</span>/Tháng</p>
                                    <p>Loại:<span class="mg-b-0 tx-medium" >`+ item.RoomTypeName + `</span> </p>
                                    <p>Trạng thái:
                                        <span class="`+ CssClass + `" name="status">
                                                `+ status + `
                                        </span>
                                        <p class="mg-b-0 mg-t-5 tx-12">Dọn vào: `+ date_into_room + `</p>
                                    </p>
                                    <p>Thu tiền:</p>
                                    <p>Điện nước:</p>
                                </div>
                            </div>
                        `;
                        //#endregion
                    }
                    $('#customize-content').hide().html(html).show('slow');
                    $('#table-list-room').hide();
                    $('#tbody-roomlist').hide();


                    TotalItem = data.Data.TotalItem;
                    TotalPage = data.Data.TotalPage;
                    load_pagination(pagination(_PageIndex, TotalPage), _PageIndex, TotalPage, TotalItem);

                    return data.Data;
                },
                error: function (xhr, status, error) {
                    return null;
                    alert("Result: " + status + " " + error + " " + xhr.status + " " + xhr.statusText)
                }
            });
    }
    //#endregion
</script>


